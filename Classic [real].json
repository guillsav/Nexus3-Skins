{
	// Front panel properties
	"panel": {

		// Here we define all colors for the panel
		"colors": {

			// General
			"background": "#e5e5e3",
			"label": "#4b4c4e",
			"accent": "#0af",
			"shadows": "#000c",
			"noise": "#00000008",

			// Groups
			"groups": {

				"background": "HSB(210,86,47)",
				"label": "HSB(203,53,100)",
				"label-dark": "HSB(207,80,63)",

				"browser": {

					"selected": {

						"background": "#37C"
					}
				}
			},

			// Dials
			"dials": {

				"ring": "#969799",

				"outline": "#0001",
				"top": "#d7d7d5",
				"bottom": "#c2bdb9",

				"bevel": {

					"highlight": "#ebebe9",
					"shadow": "#0004"
				},

				"line": "#3b3c3e"
			}
		},

		// Here we define panel-dials properties
		"dial-style": {

			"outline": 2,
			"top-bevel": 1.5,
			"ring-width": 2,
			"ring-space": 5,
			"shadow-size": 0.6,
			"shadow-distance-y": 0.3,
			"pointer-space": 3,
			"pointer-width": 2.5
		}
	},

	// Header properties
	"header": {

		// Here we define all colors for the header
		"colors": {

			// General
			"background": "#333",
			"label": "#bbb",
			"shadows": "#0008",
			"noise": "#0002",

			// Dials
			"dials": {

				"outline": "#0004",
				"ring": "#555",
				"top": "#5d5d5d",
				"bottom": "#3e3e3e",

				"bevel": {

					"highlight": "#0004",
					"shadow": "#0004"
				},

				"line": "#969799"
			}
		},

		// Here we define header-dials properties
		"dial-style": {

			"outline": 1.5
		},

		// Here we define note-event properties
		"events": {
			"note-depth": 0.2,
			"note-radius": 2,
			"note-outline": 0.5
		}
	},

	// Preview-dial
	"preview": {

		"colors": {

			"dials": {

				"outline": "#0004",
				"top": "#5d5d5d",
				"bottom": "#3e3e3e",

				"bevel": {

					"highlight": "#0004",
					"shadow": "#0004"
				},
				"line": "#969799"
			}
		},

		// Here we define preview-dial properties
		"dial-style": {

			"outline": 1,
			"ring-space": 2,	// only for preview-dial: limited to 0-2
			"ring-width": 0,	// only for preview-dial: limited to 0-2
			"shadow-size": 0.33,
			"shadow-distance-y": 0.3
		}
	},

	// Here we define the virtual keyboard
	"keyboard": {

		"colors": {
			"wheels": "#555551",
			"black-notes": "#181814",
			"white-notes": "#f8f8f4"
		},

		"realistic": true
	}
}
